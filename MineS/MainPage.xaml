<Page
    x:Class="MineS.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MineS"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel  x:Name="root" Background="{ThemeResource SystemControlAcrylicWindowMediumHighBrush}">
        <Image Source="{x:Bind local:LocalTheme.BackIM}" ></Image>
        <StackPanel x:Name="Showpp"> <TextBlock Text="MineS" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,20,0,25" FontSize="36"/>
            <local:SetPage x:Name="set" Margin="15,0,15,0"></local:SetPage>
        <Button  Grid.Row="4" Grid.ColumnSpan="2" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Content="Play" Height="50" Click="Button_Click" />
    <AppBarButton Icon="ContactInfo"/>    </StackPanel>
        
    </StackPanel>
</Page>
