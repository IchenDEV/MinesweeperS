<UserControl 
    x:Class="MineS.WinDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MineS"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
   
    
    

   >


    <StackPanel x:Name="root" Background="{ThemeResource SystemControlAcrylicElementMediumHighBrush}" HorizontalAlignment="Stretch" VerticalAlignment="Center"  >
        <TextBlock Text="Succeed" FontSize="48" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <TextBlock Text="{x:Bind Mode}" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <TextBlock FontSize="36" Text="{x:Bind Source}" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <InkCanvas x:Name="inkCanvas" Height="100" ></InkCanvas>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
             <AppBarButton Icon="Share" Click="AppBarButton_Click"/>
            <AppBarButton Icon="Save"/>
        </StackPanel>
       
    </StackPanel>
</UserControl>
